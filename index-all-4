		// IE 버전을 사용하는지 확인하는 함수
		function isIE() {
			var ua = window.navigator.userAgent;
			var isIE = /Trident\/|MSIE/.test(ua);
			return isIE;
		}
		// 사용자가 IE 버전을 사용하는지 확인
		if (isIE()) {
			alert("본 사이트는 현재 사용 중인 브라우저는 지원되지 않습니다. 최신 브라우저를 사용해 주세요.");
			$('#msie').show();
		} else {
			$('#msie').remove();
		}
		</script>
